{
  "name": "hhpack/migrate",
  "keywords": [ "lightweight", "migration", "mysql", "db"  ],
  "description": "Lightweight migration tool for Hack",
  "authors": [
    {
      "name": "Noritaka Horio",
      "email": "holy.shared.design@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "hhvm": ">= 3.18.0",
    "hhpack/publisher": "^1.0",
    "hhpack/color": "^1.0"
  },
  "require-dev": {
    "hackpack/hackunit": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "HHPack\\Migrate\\": "src/"
    },
    "files": [
      "src/bootstrap.hh"
    ]
  },
  "autoload-dev": {
    "files": [
      "test/helper/db.hh"
    ]
  },
  "scripts": {
    "test": "vendor/bin/hackunit test",
    "check": "./typecheck"
  },
  "minimum-stability": "stable"
}
