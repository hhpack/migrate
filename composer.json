{
  "name": "hhpack/migrate",
  "keywords": [ "lightweight", "migration", "mysql", "db"  ],
  "description": "Lightweight migration tool for Hack",
  "authors": [
    {
      "name": "Noritaka Horio",
      "email": "holy.shared.design@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "hhvm": ">= 3.12.0",
    "hhpack/publisher": "~0.4.0",
    "hhpack/color": "~0.7"
  },
  "require-dev": {
    "expect/peridot-expect-plugin": "~3.0",
    "peridot-php/peridot": "~1.18",
    "peridot-php/peridot-dot-reporter": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "hhpack\\migrate\\": "src/"
    },
    "files": [
      "src/bootstrap.hh"
    ]
  },
  "autoload-dev": {
    "files": [
      "spec/helper/db.hh"
    ]
  },
  "scripts": {
    "test": "vendor/bin/peridot --grep *Spec.hh -c peridot.hh spec",
    "check": "./typecheck"
  },
  "minimum-stability": "stable"
}
